<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="codeMapper">
  <insert id="selectCodeList">
    SELECT * FROM TB_CO3DE
    WHERE 1=1
    AND CODE_GRP_ID = #{codeGrpId}
  </insert>

</mapper>